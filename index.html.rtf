        );\
\
        const Send = (\{ size \}) => (\
            <svg width=\{size\} height=\{size\} viewBox="0 0 24 24" fill="none" stroke="currentColor" strokeWidth="2" strokeLinecap="round" strokeLinejoin="round">\
                <line x1="22" y1="2" x2="11" y2="13" />\
                <polygon points="22 2 15 22 11 13 2 9 22 2" />\
            </svg>\
        );\
\
        function AtlasChatbot() \{\
            const [isOpen, setIsOpen] = useState(false);\
            const [step, setStep] = useState(0);\
            const [formData, setFormData] = useState(\{\
                name: '',\
                email: '',\
                phone: '',\
                service: '',\
                timeline: ''\
            \});\
            const [messages, setMessages] = useState([\
                \{ type: 'bot', text: "Hi! Welcome to Atlas Biz Now. I'd love to learn more about your project and see how we can help you grow your business." \}\
            ]);\
            const [inputValue, setInputValue] = useState('');\
\
            const services = [\
                'Custom Website Building and Design',\
                'Creative Consultation',\
                'Powerful Lead Generation',\
                'Custom Logo Design'\
            ];\
\
            const timelines = [\
                'ASAP - Within 2 weeks',\
                '1 Month',\
                '2-3 Months',\
                'Just exploring options'\
            ];\
\
            const addMessage = (type, text) => \{\
                setMessages(prev => [...prev, \{ type, text \}]);\
            \};\
\
            const handleSubmit = () => \{\
                if (!inputValue.trim()) return;\
\
                addMessage('user', inputValue);\
\
                if (step === 0) \{\
                    setFormData(\{ ...formData, name: inputValue \});\
                    addMessage('bot', `Nice to meet you, $\{inputValue\}! What's the best email to reach you at?`);\
                    setStep(1);\
                \} else if (step === 1) \{\
                    setFormData(\{ ...formData, email: inputValue \});\
                    addMessage('bot', "Great! And what's your phone number?");\
                    setStep(2);\
                \} else if (step === 2) \{\
                    setFormData(\{ ...formData, phone: inputValue \});\
                    addMessage('bot', 'Perfect! Now, which service are you interested in?');\
                    setStep(3);\
                \}\
\
                setInputValue('');\
            \};\
\
            const handleKeyPress = (e) => \{\
                if (e.key === 'Enter') \{\
                    handleSubmit();\
                \}\
            \};\
\
            const handleServiceSelect = (service) => \{\
                setFormData(\{ ...formData, service \});\
                addMessage('user', service);\
                addMessage('bot', "Excellent choice! What's your timeline for this project?");\
                setStep(4);\
            \};\
\
            const handleTimelineSelect = (timeline) => \{\
                const completedData = \{ ...formData, timeline \};\
                setFormData(completedData);\
                addMessage('user', timeline);\
                addMessage('bot', `Thank you for sharing! Based on your interest in $\{formData.service\} with a timeline of $\{timeline\}, our team will review your information and reach out to $\{formData.email\} within 24 hours to discuss your project in detail. We're excited to help $\{formData.name\} grow their business!`);\
                setStep(5);\
                \
                console.log('Lead Captured:', completedData);\
            \};\
\
            return (\
                <div className="fixed bottom-6 right-6 z-50">\
